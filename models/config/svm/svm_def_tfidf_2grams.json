{
    "name": "svm_def_tfidf_2grams",
    "random_seed": 0,
    "tokenizer": {
        "stop": true, 
        "punct": true, 
        "lower": true, 
        "stem": false,
        "replace_nums": true
    },
    "tfidf": true,
    "vectorizer_class": "BowVectorizer",
    "vectorizer": {
        "encoding": "utf-8",
        "decode_error": "strict",
        "strip_accents": null,
        "lowercase": true,
        "preprocessor": null,
        "stop_words": null,
        "ngram_range": [1, 2],
        "analyzer": "word",
        "max_df": 1.0,
        "min_df": 2,
        "max_features": null,
        "vocabulary": null,
        "binary": false,
        "norm": "l2",
        "use_idf": true,
        "smooth_idf": true,
        "sublinear_tf": false
    },
    "model_class": "SVM",
    "model": {
        "C": 1.0, 
        "kernel": "rbf", 
        "degree": 3,
        "gamma": "scale",
        "coef0": 0.0,
        "shrinking": true,
        "probability": false,
        "tol": 0.001,
        "verbose": false,
        "max_iter": -1,
        "decision_function_shape": "ovr",
        "break_ties": false,
        "random_state": null
    }
}