{
    "name": "lr_l1_tfidf_2grams",
    "random_seed": 0,
    "tokenizer": {
        "stop": true, 
        "punct": true, 
        "lower": true, 
        "stem": false,
        "replace_nums": true
    },
    "tfidf": true,
    "vectorizer_class": "BowVectorizer",
    "vectorizer": {
        "encoding": "utf-8",
        "decode_error": "strict",
        "strip_accents": null,
        "lowercase": true,
        "preprocessor": null,
        "stop_words": null,
        "ngram_range": [1, 2],
        "analyzer": "word",
        "max_df": 1.0,
        "min_df": 2,
        "max_features": null,
        "vocabulary": null,
        "binary": false,
        "norm": "l2",
        "use_idf": true,
        "smooth_idf": true,
        "sublinear_tf": false
    },
    "model_class": "LR",
    "model": {
        "penalty": "l1", 
        "dual": false, 
        "tol": 0.0001, 
        "C": 1.0, 
        "fit_intercept": true, 
        "intercept_scaling": 1, 
        "class_weight": null, 
        "random_state": 42, 
        "solver": "liblinear", 
        "max_iter": 100, 
        "multi_class": "ovr", 
        "verbose": 0, 
        "warm_start": false, 
        "n_jobs": 1
    }
}